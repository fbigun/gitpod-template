gitConfig:
  alias.review: "log --graph --oneline --all"

tasks:
  - init: |
      sudo ln -fs /usr/share/zoneinfo/Asia/Shanghai /etc/localtime

vscode:
  extensions:
    - mhutchie.git-graph
    - EditorConfig.EditorConfig
    - esbenp.prettier-vscode
    - redjue.git-commit-plugin
    - auiworks.amvim
